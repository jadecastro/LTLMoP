# prevent system from using memprops to re-activate a deactivated action
(!m_rob2_deadlock1 & ((rob2_A_rc & rob2_A_rc' & !rob2_A) | (rob2_B_rc & rob2_B_rc' & !rob2_B) | (rob2_C_rc & rob2_C_rc' & !rob2_C) | (rob2_D1_rc & rob2_D1_rc' & !rob2_D1) | (rob2_D2_rc & rob2_D2_rc' & !rob2_D2) | (rob2_E_rc & rob2_E_rc' & !rob2_E))) -> !m_rob2_deadlock1'

(!m_rob2_deadlock2 & ((rob2_A_rc & rob2_A_rc' & rob2_A) | (rob2_B_rc & rob2_B_rc' & rob2_B) | (rob2_C_rc & rob2_C_rc' & rob2_C) | (rob2_D1_rc & rob2_D1_rc' & rob2_D1) | (rob2_D2_rc & rob2_D2_rc' & rob2_D2) | (rob2_E_rc & rob2_E_rc' & rob2_E))) -> !m_rob2_deadlock2'

# coordination - what happens when two robots deadlock at the same time?
((!m_rob1_deadlock1 | !m_rob1_deadlock2) & (!m_rob2_deadlock1 | !m_rob2_deadlock2) & (!rob12_deadlock & rob12_deadlock') & ((rob2_A_rc & rob2_A_rc') | (rob2_B_rc & rob2_B_rc') | (rob2_C_rc & rob2_C_rc') | (rob2_D1_rc & rob2_D1_rc') | (rob2_D2_rc & rob2_D2_rc') | (rob2_E_rc & rob2_E_rc')) & ((rob1_A_rc & rob1_A_rc') | (rob1_B_rc & rob1_B_rc') | (rob1_C_rc & rob1_C_rc') | (rob1_D1_rc & rob1_D1_rc') | (rob1_D2_rc & rob1_D2_rc') | (rob1_E_rc & rob1_E_rc'))) -> (((((rob1_A_rc & rob1_A_rc' & rob1_A) | (rob1_B_rc & rob1_B_rc' & rob1_B) | (rob1_C_rc & rob1_C_rc' & rob1_C) | (rob1_D1_rc & rob1_D1_rc' & rob1_D1) | (rob1_D2_rc & rob1_D2_rc' & rob1_D2) | (rob1_E_rc & rob1_E_rc' & rob1_E)) -> m_rob1_deadlock1') & (((rob1_A_rc & rob1_A_rc' & !rob1_A) | (rob1_B_rc & rob1_B_rc' & !rob1_B) | (rob1_C_rc & rob1_C_rc' & !rob1_C) | (rob1_D1_rc & rob1_D1_rc' & !rob1_D1) | (rob1_D2_rc & rob1_D2_rc' & !rob1_D2) | (rob1_E_rc & rob1_E_rc' & !rob1_E)) -> m_rob1_deadlock2')) | ((((rob2_A_rc & rob2_A_rc' & rob2_A) | (rob2_B_rc & rob2_B_rc' & rob2_B) | (rob2_C_rc & rob2_C_rc' & rob2_C) | (rob2_D1_rc & rob2_D1_rc' & rob2_D1) | (rob2_D2_rc & rob2_D2_rc' & rob2_D2) | (rob2_E_rc & rob2_E_rc' & rob2_E)) -> m_rob2_deadlock1') & (((rob2_A_rc & rob2_A_rc' & !rob2_A) | (rob2_B_rc & rob2_B_rc' & !rob2_B) | (rob2_C_rc & rob2_C_rc' & !rob2_C) | (rob2_D1_rc & rob2_D1_rc' & !rob2_D1) | (rob2_D2_rc & rob2_D2_rc' & !rob2_D2) | (rob2_E_rc & rob2_E_rc' & !rob2_E)) -> m_rob2_deadlock2')))


((!m_rob1_deadlock1 | !m_rob1_deadlock2) & (m_rob2_deadlock1 & m_rob2_deadlock2) & (!rob12_deadlock & rob12_deadlock') & ((rob2_A_rc & rob2_A_rc') | (rob2_B_rc & rob2_B_rc') | (rob2_C_rc & rob2_C_rc') | (rob2_D1_rc & rob2_D1_rc') | (rob2_D2_rc & rob2_D2_rc') | (rob2_E_rc & rob2_E_rc')) & ((rob1_A_rc & rob1_A_rc') | (rob1_B_rc & rob1_B_rc') | (rob1_C_rc & rob1_C_rc') | (rob1_D1_rc & rob1_D1_rc') | (rob1_D2_rc & rob1_D2_rc') | (rob1_E_rc & rob1_E_rc'))) -> ((((rob1_A_rc & rob1_A_rc' & rob1_A) | (rob1_B_rc & rob1_B_rc' & rob1_B) | (rob1_C_rc & rob1_C_rc' & rob1_C) | (rob1_D1_rc & rob1_D1_rc' & rob1_D1) | (rob1_D2_rc & rob1_D2_rc' & rob1_D2) | (rob1_E_rc & rob1_E_rc' & rob1_E)) -> m_rob1_deadlock1') & (((rob1_A_rc & rob1_A_rc' & !rob1_A) | (rob1_B_rc & rob1_B_rc' & !rob1_B) | (rob1_C_rc & rob1_C_rc' & !rob1_C) | (rob1_D1_rc & rob1_D1_rc' & !rob1_D1) | (rob1_D2_rc & rob1_D2_rc' & !rob1_D2) | (rob1_E_rc & rob1_E_rc' & !rob1_E)) -> m_rob1_deadlock2'))

((m_rob1_deadlock1 & m_rob1_deadlock2) & (!m_rob2_deadlock1 | !m_rob2_deadlock2) & (!rob12_deadlock & rob12_deadlock') & ((rob2_A_rc & rob2_A_rc') | (rob2_B_rc & rob2_B_rc') | (rob2_C_rc & rob2_C_rc') | (rob2_D1_rc & rob2_D1_rc') | (rob2_D2_rc & rob2_D2_rc') | (rob2_E_rc & rob2_E_rc')) & ((rob1_A_rc & rob1_A_rc') | (rob1_B_rc & rob1_B_rc') | (rob1_C_rc & rob1_C_rc') | (rob1_D1_rc & rob1_D1_rc') | (rob1_D2_rc & rob1_D2_rc') | (rob1_E_rc & rob1_E_rc'))) -> ((((rob2_A_rc & rob2_A_rc' & rob2_A) | (rob2_B_rc & rob2_B_rc' & rob2_B) | (rob2_C_rc & rob2_C_rc' & rob2_C) | (rob2_D1_rc & rob2_D1_rc' & rob2_D1) | (rob2_D2_rc & rob2_D2_rc' & rob2_D2) | (rob2_E_rc & rob2_E_rc' & rob2_E)) -> m_rob2_deadlock1') & (((rob2_A_rc & rob2_A_rc' & !rob2_A) | (rob2_B_rc & rob2_B_rc' & !rob2_B) | (rob2_C_rc & rob2_C_rc' & !rob2_C) | (rob2_D1_rc & rob2_D1_rc' & !rob2_D1) | (rob2_D2_rc & rob2_D2_rc' & !rob2_D2) | (rob2_E_rc & rob2_E_rc' & !rob2_E)) -> m_rob2_deadlock2'))

(!m_rob1_deadlock1 & ((!rob1_deadlock & !rob1_deadlock') & (!rob12_deadlock & !rob12_deadlock'))) -> (!m_rob1_deadlock1')
(!m_rob1_deadlock2 & ((!rob1_deadlock & !rob1_deadlock') & (!rob12_deadlock & !rob12_deadlock'))) -> (!m_rob1_deadlock2')
(!m_rob2_deadlock1 & ((!rob2_deadlock & !rob2_deadlock') & (!rob12_deadlock & !rob12_deadlock'))) -> (!m_rob2_deadlock1')
(!m_rob2_deadlock2 & ((!rob2_deadlock & !rob2_deadlock') & (!rob12_deadlock & !rob12_deadlock'))) -> (!m_rob2_deadlock2')


