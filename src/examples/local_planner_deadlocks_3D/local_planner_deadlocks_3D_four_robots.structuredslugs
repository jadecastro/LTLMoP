rob2_B -> ! (rob2_A | rob2_C | rob2_D1 | rob2_D2 | rob2_E)
rob2_C -> ! (rob2_A | rob2_B | rob2_D1 | rob2_D2 | rob2_E)
rob2_D1 -> ! (rob2_A | rob2_B | rob2_C | rob2_D2 | rob2_E)
rob2_D2 -> ! (rob2_A | rob2_B | rob2_C | rob2_D1 | rob2_E)
