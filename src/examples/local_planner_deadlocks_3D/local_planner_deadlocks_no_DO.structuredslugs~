# set memory of deadlock when it has occured, resetting upon entering a new region
(!m_rob1_deadlock & ((rob1_T_rc & rob1_T_rc') | (rob1_B_rc & rob1_B_rc') | (rob1_L_rc & rob1_L_rc') | (rob1_R_rc & rob1_R_rc') | (rob1_D_rc & rob1_D_rc'))) -> ((rob1_deadlock & !rob1_deadlock') <-> m_rob1_deadlock')

(m_rob1_deadlock & ((rob1_T_rc & rob1_T_rc') | (rob1_B_rc & rob1_B_rc') | (rob1_L_rc & rob1_L_rc') | (rob1_R_rc & rob1_R_rc') | (rob1_D_rc & rob1_D_rc'))) -> m_rob1_deadlock'

