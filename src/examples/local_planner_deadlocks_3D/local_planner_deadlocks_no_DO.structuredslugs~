# REGION T
(m_rob2_deadlock & ((rob2_T_rc & rob2_T_rc' & rob2_T & rob2_T') | (rob2_B_rc & rob2_B_rc' & rob2_B & rob2_B') | (rob2_L_rc & rob2_L_rc' & rob2_L & rob2_L') | (rob2_R_rc & rob2_R_rc' & rob2_R & rob2_R') | (rob2_D_rc & rob2_D_rc' & rob2_D & rob2_D')) & !m_rob1_deadlock & m_rob1_deadlock' & rob2_T_rc & rob2_T_rc' & rob2_L) -> (!rob2_L')
(m_rob2_deadlock & ((rob2_T_rc & rob2_T_rc' & rob2_T & rob2_T') | (rob2_B_rc & rob2_B_rc' & rob2_B & rob2_B') | (rob2_L_rc & rob2_L_rc' & rob2_L & rob2_L') | (rob2_R_rc & rob2_R_rc' & rob2_R & rob2_R') | (rob2_D_rc & rob2_D_rc' & rob2_D & rob2_D')) & !m_rob1_deadlock & m_rob1_deadlock' & rob2_T_rc & rob2_T_rc' & rob2_R) -> (!rob2_R')

