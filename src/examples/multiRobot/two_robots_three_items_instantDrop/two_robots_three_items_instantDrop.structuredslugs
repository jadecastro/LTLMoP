# how to pick up etc
(rob1_metal' & ! rob1_carrying_metal & ! rob1_carrying_glass & ! rob1_carrying_paper) -> rob1_pickup'
((rob1_carrying_metal & ! (rob1_r6_rc' & rob1_drop')) | (rob1_metal' & rob1_pickup_ac')) -> rob1_carrying_metal'
rob1_carrying_metal' -> ((rob1_carrying_metal & ! (rob1_r6_rc' & rob1_drop')) | (rob1_metal' & rob1_pickup_ac'))

(rob1_glass' & ! rob1_carrying_metal & ! rob1_carrying_glass & ! rob1_carrying_paper) -> rob1_pickup'
((rob1_carrying_glass & ! (rob1_r8_rc' & rob1_drop')) | (rob1_glass' & rob1_pickup_ac')) -> rob1_carrying_glass'
rob1_carrying_glass' -> ((rob1_carrying_glass & ! (rob1_r8_rc' & rob1_drop')) | (rob1_glass' & rob1_pickup_ac'))

(rob1_paper' & ! rob1_carrying_metal & ! rob1_carrying_glass & ! rob1_carrying_paper) -> rob1_pickup'
((rob1_carrying_paper & ! (rob1_r3_rc' & rob1_drop')) | (rob1_paper' & rob1_pickup_ac')) -> rob1_carrying_paper'
rob1_carrying_paper' -> ((rob1_carrying_paper & ! (rob1_r3_rc' & rob1_drop')) | (rob1_paper' & rob1_pickup_ac'))

(! ((rob1_carrying_metal & rob1_r6_rc') | (rob1_carrying_glass & rob1_r8_rc') | (rob1_carrying_paper & rob1_r3_rc')) -> ! rob1_drop')
! (rob1_metal' | rob1_glass' | rob1_paper') -> ! rob1_pickup'

